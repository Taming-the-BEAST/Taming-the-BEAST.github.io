                        <sequence id="seq_Pan" taxon="Pan" totalcount="4" value="-CGACCCCTTATTTACCGAGAAAGCT-"/>
                        <sequence id="seq_Gorilla" taxon="Gorilla" totalcount="4" value="-CAACCCCTTATTTACCGAGAAAGCT-"/>
                        <sequence id="seq_Pongo" taxon="Pongo" totalcount="4" value="-CAACCCCTTATTTACCGAGAAAGCT-CACAAGAACTGCTAACTCTCACT-"/>
                        <sequence id="seq_Hylobates" taxon="Hylobates" totalcount="4" value="-AAACCTCTTGCTTACCGAGAAAGCC-"/>
                        <sequence id="seq_Macaca_fuscata" taxon="Macaca_fuscata" totalcount="4" value="-CCACCTCTTATTTACCGAGAAAACT-"/>
                        <sequence id="seq_M._mulatta" taxon="M._mulatta" totalcount="4" value="-CCACCTCTTATTTACCGAGAAAACT-"/>
                        <sequence id="seq_M._fascicularis" taxon="M._fascicularis" totalcount="4" value="-CCACTTCTTATTTACCGAGAAAACT-"/>
                        <sequence id="seq_M._sylvanus" taxon="M._sylvanus" totalcount="4" value="-CCACTTCTTATTTACCGAGAAAACT-"/>
